{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": { "scripts": ["background.js"], "css":"background.css" },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}